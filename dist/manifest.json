{
  "manifest_version": 3,
  "name": "wp ext",
  "version": "1.0",
  "description": "Testin ts and react extension",
  "action": {
    "default_popup": "popup.html",
    "default_title": "TS React",
    "default_action": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },

  "web_accessible_resources": [
    {
      "resources": ["icon.png", "nodes.svg", "f5b5c50e1991f79f8733.svg", "6bcffb56bd0efaa47767.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
